{
  "name": "sb2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.4",
    "@types/node": "11.9.4",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.1",
    "gridlex": "^2.7.1",
    "normalize-scss": "^7.0.1",
    "onchange": "^5.0.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-scripts": "2.1.5",
    "typescript": "3.3.3"
  },
  "scripts": {
    "start": "parcel src/dev/index.html",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "css:compile": "node-sass src/ -o src/",
    "css:autoprefix": "postcss -u autoprefixer -r ./src/styles/app.css",
    "css:build": "npm run css:compile | npm run css:autoprefix",
    "css:watch": "npm run css:build && onchange ./src/styles/**/*.scss ./src/styles/*.scss -- npm run css:build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-info": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "@types/storybook__react": "^4.0.1",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "expect": "^24.8.0",
    "node-sass": "^4.12.0",
    "parcel-bundler": "^1.12.3",
    "postcss": "^7.0.16",
    "postcss-cli": "^6.1.2",
    "react-docgen-typescript-loader": "^3.0.1",
    "storybook-addon-specifications": "^2.1.2"
  }
}
